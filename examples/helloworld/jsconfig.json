{
  "compilerOptions": {
    "checkJs": false,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["**/*.js", "**/*.mjs"],
  "exclude": ["node_modules/**"]
}
