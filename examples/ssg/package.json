{
  "name": "@raven-js/ssg-example",
  "version": "1.0.0",
  "description": "Static Site Generator example using RavenJS building blocks",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node --watch-path=./src cfg/start.js",
    "build": "node ../../packages/fledge/bin/fledge.js static ./raven.config.js:build",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint ."
  },
  "dependencies": {
    "@raven-js/wings": "*",
    "@raven-js/beak": "*",
    "@raven-js/reflex": "*",
    "@raven-js/cortex": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0"
  }
}
