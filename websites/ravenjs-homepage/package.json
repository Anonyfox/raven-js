{
  "name": "ravenjs-homepage",
  "version": "1.0.0",
  "description": "Official RavenJS homepage built with RavenJS toolkit",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node --watch-path=./src start.js",
    "build": "npx @raven-js/fledge static ./raven.config.js:build",
    "deploy": "npm run build && npx @raven-js/soar deploy --static ./dist --cf-workers ravenjs-homepage",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint ."
  },
  "dependencies": {
    "@raven-js/wings": "*",
    "@raven-js/beak": "*",
    "@raven-js/reflex": "*",
    "@raven-js/cortex": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0"
  }
}
